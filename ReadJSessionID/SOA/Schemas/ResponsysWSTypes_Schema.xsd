<?xml version= '1.0' encoding= 'UTF-8' ?>
<schema elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
     targetNamespace="urn:ws.rsys.com" xmlns:tns="urn:ws.rsys.com" xmlns:fns="urn:fault.ws.rsys.com">
    <import namespace="urn:fault.ws.rsys.com" xmlns:fns="urn:fault.ws.rsys.com" schemaLocation="ResponsysWSFaults_Schema.xsd"/>
    <element name="login">
        <complexType>
            <sequence>
                <element name="username" nillable="true" type="xsd:string"/>
                <element name="password" nillable="true" type="xsd:string"/>
            </sequence>
        </complexType>
    </element>
    <complexType name="LoginResult">
        <sequence>
            <element name="sessionId" nillable="true" type="xsd:string"/>
        </sequence>
    </complexType>
    <element name="loginResponse">
        <complexType>
            <sequence>
                <element name="result" nillable="true" type="tns:LoginResult"/>
            </sequence>
        </complexType>
    </element>
    <complexType name="ServerAuthResult">
        <sequence>
            <element name="authSessionId" nillable="true" type="xsd:string"/>
            <element maxOccurs="unbounded" name="encryptedClientChallenge" nillable="true" type="xsd:byte"/>
            <element maxOccurs="unbounded" name="serverChallenge" nillable="true" type="xsd:byte"/>
        </sequence>
    </complexType>
    <element name="authenticateServer">
        <complexType>
            <sequence>
                <element name="username" nillable="true" type="xsd:string"/>
                <element maxOccurs="unbounded" name="clientChallenge" nillable="true" type="xsd:byte"/>
            </sequence>
        </complexType>
    </element>
    <element name="authenticateServerResponse">
        <complexType>
            <sequence>
                <element name="result" type="tns:ServerAuthResult"/>
            </sequence>
        </complexType>
    </element>
    <element name="loginWithCertificate">
        <complexType>
            <sequence>
                <element maxOccurs="unbounded" name="encryptedServerChallenge" nillable="true" type="xsd:byte"/>
            </sequence>
        </complexType>
    </element>
    <element name="loginWithCertificateResponse">
        <complexType>
            <sequence>
                <element name="result" type="tns:LoginResult"/>
            </sequence>
        </complexType>
    </element>
    <element name="logout">
        <complexType/>
    </element>
    <element name="logoutResponse">
        <complexType>
            <sequence>
                <element name="result" type="xsd:boolean"/>
            </sequence>
        </complexType>
    </element>
    <element name="createFolder">
        <complexType>
            <sequence>
                <element name="folderName" type="xsd:string"/>
            </sequence>
        </complexType>
    </element>
    <element name="createFolderResponse">
        <complexType>
            <sequence>
                <element name="result" type="xsd:boolean"/>
            </sequence>
        </complexType>
    </element>
    <element name="deleteFolder">
        <complexType>
            <sequence>
                <element name="folderName" type="xsd:string"/>
            </sequence>
        </complexType>
    </element>
    <element name="deleteFolderResponse">
        <complexType>
            <sequence>
                <element name="result" type="xsd:boolean"/>
            </sequence>
        </complexType>
    </element>
    <element name="listFolders">
        <complexType/>
    </element>
    <complexType name="FolderResult">
        <sequence>
            <element name="name" type="xsd:string"/>
        </sequence>
    </complexType>
    <element name="listFoldersResponse">
        <complexType>
            <sequence>
                <element maxOccurs="unbounded" name="result" nillable="true" type="tns:FolderResult"/>
            </sequence>
        </complexType>
    </element>
    <simpleType name="FolderObjectType">
        <restriction base="xsd:string">
            <enumeration value="ALL_TYPES"/>
            <enumeration value="PROGRAM"/>
            <enumeration value="CAMPAIGN"/>
            <enumeration value="FORM"/>
            <enumeration value="LIST"/>
            <enumeration value="LIST_FILTER"/>
            <enumeration value="SUPPLEMENTAL_TABLE"/>
            <enumeration value="SUPPLEMENTAL_FILTER"/>
            <enumeration value="SUPPLEMENTAL_JOIN"/>
            <enumeration value="SUPPLEMENTAL_SQL"/>
            <enumeration value="LINK_TABLE"/>
            <enumeration value="DOCUMENT"/>
            <enumeration value="ATTACHMENT"/>
        </restriction>
    </simpleType>
    <element name="listFolderObjects">
        <complexType>
            <sequence>
                <element name="folderName" nillable="true" type="xsd:string"/>
                <element name="type" nillable="true" type="tns:FolderObjectType"/>
            </sequence>
        </complexType>
    </element>
    <complexType name="InteractObject">
        <sequence>
            <element name="folderName" type="xsd:string"/>
            <element name="objectName" type="xsd:string"/>
        </sequence>
    </complexType>
    <complexType name="FolderObjectResult">
        <sequence>
            <element name="id" type="xsd:long"/>
            <element name="name" type="tns:InteractObject"/>
            <element name="type" type="tns:FolderObjectType"/>
            <element name="subType" type="xsd:string"/>
        </sequence>
    </complexType>
    <element name="listFolderObjectsResponse">
        <complexType>
            <sequence>
                <element maxOccurs="unbounded" name="result" nillable="true" type="tns:FolderObjectResult"/>
            </sequence>
        </complexType>
    </element>

<!-- Header Elements 
-->
    <element name="SessionHeader">
        <complexType>
            <sequence>
                <element name="sessionId" type="xsd:string"/>
            </sequence>
        </complexType>
    </element>
    <element name="AuthSessionHeader">
        <complexType>
            <sequence>
                <element name="authSessionId" type="xsd:string"/>
            </sequence>
        </complexType>
    </element>

<!-- Header Elements 
-->
    <simpleType name="EmailFormat">
        <restriction base="xsd:string">
            <enumeration value="TEXT_FORMAT"/>
            <enumeration value="HTML_FORMAT"/>
            <enumeration value="MULTIPART_FORMAT"/>
            <enumeration value="NO_FORMAT"/>
        </restriction>
    </simpleType>
    <complexType name="Recipient">
        <sequence>
            <element name="listName" nillable="true" type="tns:InteractObject"/>
            <element name="recipientId" nillable="true" type="xsd:long"/>
            <element name="customerId" nillable="true" type="xsd:string"/>
            <element name="emailAddress" nillable="true" type="xsd:string"/>
            <element name="mobileNumber" nillable="true" type="xsd:string"/>
            <element name="emailFormat" nillable="true" type="tns:EmailFormat"/>
        </sequence>
    </complexType>
    <simpleType name="RecipientLocale">
        <restriction base="xsd:string">
            <enumeration value="en"/>
            <enumeration value="en_US_ASCII"/>
            <enumeration value="nl"/>
            <enumeration value="fr"/>
            <enumeration value="de"/>
            <enumeration value="it"/>
            <enumeration value="pt"/>
            <enumeration value="es"/>
            <enumeration value="et"/>
            <enumeration value="lv"/>
            <enumeration value="lt"/>
            <enumeration value="sq"/>
            <enumeration value="hr"/>
            <enumeration value="cs"/>
            <enumeration value="hu"/>
            <enumeration value="pl"/>
            <enumeration value="ro"/>
            <enumeration value="sk"/>
            <enumeration value="sl"/>
            <enumeration value="zh"/>
            <enumeration value="zh_TW"/>
            <enumeration value="el"/>
            <enumeration value="ja"/>
            <enumeration value="ko"/>
            <enumeration value="ru"/>
            <enumeration value="ru__cp1251"/>
            <enumeration value="tr"/>
            <enumeration value="da"/>
            <enumeration value="fi"/>
            <enumeration value="is"/>
            <enumeration value="sv"/>
        </restriction>
    </simpleType>
    <simpleType name="CharacterEncoding">
        <restriction base="xsd:string">
            <enumeration value="ISO_8859_1"/>
            <enumeration value="windows_1257"/>
            <enumeration value="ISO_8859_2"/>
            <enumeration value="gb2312"/>
            <enumeration value="big5"/>
            <enumeration value="ISO_8859_7"/>
            <enumeration value="SJIS"/>
            <enumeration value="euc_kr"/>
            <enumeration value="koi8_r"/>
            <enumeration value="ISO_8859_9"/>
            <enumeration value="UTF_8"/>
        </restriction>
    </simpleType>
    <simpleType name="CampaignType">
        <restriction base="xsd:string">
            <enumeration value="EMAIL"/>
            <enumeration value="MOBILE"/>
        </restriction>
    </simpleType>
    <complexType name="OptionalData">
        <sequence>
            <element name="name" nillable="true" type="xsd:string"/>
            <element name="value" nillable="true" type="xsd:string"/>
        </sequence>
    </complexType>
    <simpleType name="UnsubscribeOption">
        <restriction base="xsd:string">
            <enumeration value="NO_OPTOUT_BUTTON"/>
            <enumeration value="OPTOUT_SINGLE_CLICK"/>
            <enumeration value="OPTOUT_FORM"/>
        </restriction>
    </simpleType>
    <simpleType name="AutoCloseOption">
        <restriction base="xsd:string">
            <enumeration value="NO_AUTO_CLOSE"/>
            <enumeration value="AUTO_CLOSE_X_DAYS_AFTER_LAUNCH"/>
            <enumeration value="AUTO_CLOSE_X_DAYS_AFTER_LAST_RESPONSE"/>
            <enumeration value="AUTO_CLOSE_ON_DATE"/>
        </restriction>
    </simpleType>
    <simpleType name="CampaignPurpose">
        <restriction base="xsd:string">
            <enumeration value="PROMOTIONAL"/>
            <enumeration value="TRANSACTIONAL"/>
        </restriction>
    </simpleType>
    <complexType name="CampaignProperties">
        <sequence>
            <element name="campaign" nillable="true" type="tns:InteractObject"/>
            <element name="campaignId" nillable="true" type="xsd:long"/>
            <element name="campaignType" nillable="true" type="tns:CampaignType"/>
            <element name="description" nillable="true" type="xsd:string"/>
            <element name="marketingProgram" nillable="true" type="xsd:string"/>
            <element name="marketingStrategy" nillable="true" type="xsd:string"/>
            <element name="list" nillable="true" type="tns:InteractObject"/>
            <element maxOccurs="unbounded" name="refiningFilters" nillable="true" type="tns:InteractObject"/>
            <element name="refiningDataSource" nillable="true" type="tns:InteractObject"/>
            <element name="governanceFilter" nillable="true" type="tns:InteractObject"/>
            <element maxOccurs="unbounded" name="supplementalCampaignDataSources" nillable="true"
                 type="tns:InteractObject"/>
            <element maxOccurs="unbounded" name="suppressionLists" nillable="true" type="tns:InteractObject"/>
            <element maxOccurs="unbounded" name="supplementalProofDataSources" nillable="true" type="tns:InteractObject"/>
            <element maxOccurs="unbounded" name="supplementalSeedDataSources" nillable="true" type="tns:InteractObject"/>
            <element name="proofList" nillable="true" type="tns:InteractObject"/>
            <element name="seedList" nillable="true" type="tns:InteractObject"/>
            <element maxOccurs="unbounded" name="segmentation" nillable="true" type="tns:InteractObject"/>

<!-- Content 
-->
            <element name="fromName" nillable="true" type="xsd:string"/>
            <element name="fromEmail" nillable="true" type="xsd:string"/>
            <element name="replyToEmail" nillable="true" type="xsd:string"/>
            <element name="bccEmail" nillable="true" type="xsd:string"/>
            <element name="subject" nillable="true" type="xsd:string"/>

<!-- Body 
-->
            <element name="htmlMessage" nillable="true" type="tns:InteractObject"/>
            <element name="textMessage" nillable="true" type="tns:InteractObject"/>

<!-- Link Tracking 
-->
            <element name="enableLinkTracking" nillable="true" type="xsd:boolean"/>
            <element name="linkTable" nillable="true" type="tns:InteractObject"/>

<!-- External Tracking 
-->
            <element name="enableExternalTracking" nillable="true" type="xsd:boolean"/>
            <element maxOccurs="unbounded" name="externalTrackingParams" nillable="true" type="tns:OptionalData"/>

<!-- Attachments 
-->
            <element maxOccurs="unbounded" name="attachments" nillable="true" type="tns:InteractObject"/>

<!-- Campaign Variables 
-->
            <element maxOccurs="unbounded" name="campaignVariables" nillable="true" type="tns:OptionalData"/>

<!-- Settings 
-->
            <element name="campaignPurpose" nillable="true" type="tns:CampaignPurpose"/>
            <element name="useUTF8" nillable="true" type="xsd:boolean"/>
            <element name="locale" nillable="true" type="tns:RecipientLocale"/>
            <element name="trackHtmlOpens" nillable="true" type="xsd:boolean"/>
            <element name="trackConversions" nillable="true" type="xsd:boolean"/>
            <element name="sendTextIfHTMLUnknown" nillable="true" type="xsd:boolean"/>
            <element name="segmentTrackingColumnName" nillable="true" type="xsd:string"/>
            <element name="unsubscribeOption" nillable="true" type="tns:UnsubscribeOption"/>
            <element name="unsubscribeForm" nillable="true" type="xsd:string"/>
            <element name="autoCloseOption" nillable="true" type="tns:AutoCloseOption"/>
            <element name="autoCloseValue" nillable="true" type="xsd:string"/>
            <element name="closedCampaignURL" nillable="true" type="xsd:string"/>
            <element name="externalCampaignCode" nillable="true" type="xsd:string"/>
            <element name="salesForceCampaignId" nillable="true" type="xsd:string"/>
        </sequence>
    </complexType>
    <complexType name="RecipientData">
        <sequence>
            <element name="recipient" nillable="true" type="tns:Recipient"/>
            <element maxOccurs="unbounded" name="optionalData" nillable="true" type="tns:OptionalData"/>
        </sequence>
    </complexType>
    <element name="triggerCampaignMessage">
        <complexType>
            <sequence>
                <element name="campaign" nillable="true" type="tns:InteractObject"/>
                <element maxOccurs="unbounded" name="recipientData" nillable="true" type="tns:RecipientData"/>
            </sequence>
        </complexType>
    </element>
    <complexType name="TriggerResult">
        <sequence>
            <element name="recipientId" nillable="true" type="xsd:long"/>
            <element name="success" nillable="true" type="xsd:boolean"/>
            <element name="errorMessage" nillable="true" type="xsd:string"/>
        </sequence>
    </complexType>
    <element name="triggerCampaignMessageResponse">
        <complexType>
            <sequence>
                <element maxOccurs="unbounded" name="result" nillable="true" type="tns:TriggerResult"/>
            </sequence>
        </complexType>
    </element>
    <complexType name="TriggerData">
        <sequence>
            <element maxOccurs="unbounded" name="optionalData" nillable="true" type="tns:OptionalData"/>
        </sequence>
    </complexType>
    <element name="HaMergeTriggerEmail">
        <complexType>
            <sequence>
                <element name="recordData" nillable="true" type="tns:RecordData"/>
                <element name="mergeRule" nillable="true" type="tns:ListMergeRule"/>
                <element name="campaign" nillable="true" type="tns:InteractObject"/>
                <element maxOccurs="unbounded" name="triggerData" nillable="true" type="tns:TriggerData"/>
            </sequence>
        </complexType>
    </element>
    <element name="HaMergeTriggerEmailResponse">
        <complexType>
            <sequence>
                <element maxOccurs="unbounded" name="result" nillable="true" type="tns:TriggerResult"/>
            </sequence>
        </complexType>
    </element>
    <element name="mergeTriggerEmail">
        <complexType>
            <sequence>
                <element name="recordData" nillable="true" type="tns:RecordData"/>
                <element name="mergeRule" nillable="true" type="tns:ListMergeRule"/>
                <element name="campaign" nillable="true" type="tns:InteractObject"/>
                <element maxOccurs="unbounded" name="triggerData" nillable="true" type="tns:TriggerData"/>
            </sequence>
        </complexType>
    </element>
    <element name="mergeTriggerEmailResponse">
        <complexType>
            <sequence>
                <element maxOccurs="unbounded" name="result" nillable="true" type="tns:TriggerResult"/>
            </sequence>
        </complexType>
    </element>
    <complexType name="CustomEvent">
        <sequence>
            <element name="eventName" nillable="true" type="xsd:string"/>
            <element name="eventId" nillable="true" type="xsd:long"/>
            <element name="eventStringDataMapping" nillable="true" type="xsd:string"/>
            <element name="eventDateDataMapping" nillable="true" type="xsd:string"/>
            <element name="eventNumberDataMapping" nillable="true" type="xsd:string"/>
        </sequence>
    </complexType>
    <element name="triggerCustomEvent">
        <complexType>
            <sequence>
                <element name="customEvent" type="tns:CustomEvent"/>
                <element maxOccurs="unbounded" name="recipientData" nillable="true" type="tns:RecipientData"/>
            </sequence>
        </complexType>
    </element>
    <element name="triggerCustomEventResponse">
        <complexType>
            <sequence>
                <element maxOccurs="unbounded" name="result" nillable="true" type="tns:TriggerResult"/>
            </sequence>
        </complexType>
    </element>
    <element name="createCampaign">
        <complexType>
            <sequence>
                <element name="campaign" nillable="true" type="tns:InteractObject"/>
                <element name="campaignType" nillable="true" type="tns:CampaignType"/>
            </sequence>
        </complexType>
    </element>
    <element name="createCampaignResponse">
        <complexType>
            <sequence>
                <element name="result" type="xsd:long"/>
            </sequence>
        </complexType>
    </element>
    <element name="setCampaignProperties">
        <complexType>
            <sequence>
                <element name="campaignProperties" type="tns:CampaignProperties"/>
            </sequence>
        </complexType>
    </element>
    <element name="setCampaignPropertiesResponse">
        <complexType>
            <sequence>
                <element name="result" type="xsd:boolean"/>
            </sequence>
        </complexType>
    </element>
    <element name="getCampaignProperties">
        <complexType>
            <sequence>
                <element name="campaign" nillable="true" type="tns:InteractObject"/>
                <element name="campaignId" nillable="true" type="xsd:long"/>
            </sequence>
        </complexType>
    </element>
    <element name="getCampaignPropertiesResponse">
        <complexType>
            <sequence>
                <element name="result" nillable="true" type="tns:CampaignProperties"/>
            </sequence>
        </complexType>
    </element>
    <simpleType name="ProofLaunchType">
        <restriction base="xsd:string">
            <enumeration value="LAUNCH_TO_ADDRESS"/>
            <enumeration value="LAUNCH_TO_PROOFLIST"/>
            <enumeration value="LAUNCH_TO_ADDRESS_USING_PROOFLIST"/>
        </restriction>
    </simpleType>
    <simpleType name="ProgressChunk">
        <restriction base="xsd:string">
            <enumeration value="CHUNK_10K"/>
            <enumeration value="CHUNK_50K"/>
            <enumeration value="CHUNK_100K"/>
            <enumeration value="CHUNK_500K"/>
            <enumeration value="CHUNK_1M"/>
        </restriction>
    </simpleType>
    <complexType name="ProofLaunchOptions">
        <sequence>
            <element name="proofEmailAddress" nillable="true" type="xsd:string"/>
            <element name="proofLaunchType" nillable="true" type="tns:ProofLaunchType"/>
        </sequence>
    </complexType>
    <complexType name="LaunchPreferences">
        <sequence>
            <element name="enableLimit" nillable="true" type="xsd:boolean"/>
            <element name="recipientLimit" nillable="true" type="xsd:long"/>
            <element name="enableNthSampling" nillable="true" type="xsd:boolean"/>
            <element name="samplingNthSelection" nillable="true" type="xsd:int"/>
            <element name="samplingNthInterval" nillable="true" type="xsd:int"/>
            <element name="samplingNthOffset" nillable="true" type="xsd:int"/>
            <element name="enableProgressAlerts" nillable="true" type="xsd:boolean"/>
            <element name="progressEmailAddresses" nillable="true" type="xsd:string"/>
            <element name="progressChunk" nillable="true" type="tns:ProgressChunk"/>
        </sequence>
    </complexType>
    <complexType name="LaunchResult">
        <sequence>
            <element name="launchId" nillable="true" type="xsd:long"/>
        </sequence>
    </complexType>
    <complexType name="LaunchStatusResult">
        <sequence>
            <element name="launchId" nillable="true" type="xsd:long"/>
            <element name="launchState" nillable="true" type="xsd:string"/>
            <element name="launchType" nillable="true" type="xsd:string"/>
            <element name="launchDate" nillable="true" type="xsd:dateTime"/>
            <element name="campaign" nillable="true" type="xsd:string"/>
        </sequence>
    </complexType>
    <element name="launchCampaign">
        <complexType>
            <sequence>
                <element name="campaign" nillable="true" type="tns:InteractObject"/>
                <element name="proofLaunchOptions" nillable="true" type="tns:ProofLaunchOptions"/>
                <element name="launchPreferences" nillable="true" type="tns:LaunchPreferences"/>
            </sequence>
        </complexType>
    </element>
    <element name="launchCampaignResponse">
        <complexType>
            <sequence>
                <element name="result" nillable="true" type="tns:LaunchResult"/>
            </sequence>
        </complexType>
    </element>
    <element name="scheduleCampaignLaunch">
        <complexType>
            <sequence>
                <element name="campaign" nillable="true" type="tns:InteractObject"/>
                <element name="proofLaunchOptions" nillable="true" type="tns:ProofLaunchOptions"/>
                <element name="launchPreferences" nillable="true" type="tns:LaunchPreferences"/>
                <element name="scheduleDate" nillable="true" type="xsd:dateTime"/>
            </sequence>
        </complexType>
    </element>
    <element name="scheduleCampaignLaunchResponse">
        <complexType>
            <sequence>
                <element name="result" type="xsd:boolean"/>
            </sequence>
        </complexType>
    </element>
    <element name="getLaunchStatus">
        <complexType>
            <sequence>
                <element maxOccurs="unbounded" name="launchIds" nillable="true" type="xsd:long"/>
            </sequence>
        </complexType>
    </element>
    <element name="getLaunchStatusResponse">
        <complexType>
            <sequence>
                <element maxOccurs="unbounded" name="result" nillable="true" type="tns:LaunchStatusResult"/>
            </sequence>
        </complexType>
    </element>
    <element name="getCampaignId">
        <complexType>
            <sequence>
                <element name="campaign" nillable="true" type="tns:InteractObject"/>
            </sequence>
        </complexType>
    </element>
    <element name="getCampaignIdResponse">
        <complexType>
            <sequence>
                <element name="result" type="xsd:long"/>
            </sequence>
        </complexType>
    </element>
    <element name="deleteCampaign">
        <complexType>
            <sequence>
                <element name="campaign" nillable="true" type="tns:InteractObject"/>
            </sequence>
        </complexType>
    </element>
    <element name="deleteCampaignResponse">
        <complexType>
            <sequence>
                <element name="result" type="xsd:boolean"/>
            </sequence>
        </complexType>
    </element>
    <element name="getLaunchesForCampaign">
        <complexType>
            <sequence>
                <element name="campaign" nillable="true" type="tns:InteractObject"/>
                <element name="campaignId" nillable="true" type="xsd:long"/>
                <element name="fromDate" nillable="true" type="xsd:dateTime"/>
                <element name="toDate" nillable="true" type="xsd:dateTime"/>
            </sequence>
        </complexType>
    </element>
    <element name="getLaunchesForCampaignResponse">
        <complexType>
            <sequence>
                <element maxOccurs="unbounded" name="result" nillable="true" type="tns:LaunchStatusResult"/>
            </sequence>
        </complexType>
    </element>
    <element name="getOpenLaunches">
        <complexType/>
    </element>
    <element name="getOpenLaunchesResponse">
        <complexType>
            <sequence>
                <element maxOccurs="unbounded" name="result" nillable="true" type="tns:LaunchStatusResult"/>
            </sequence>
        </complexType>
    </element>
    <element name="closeCampaign">
        <complexType>
            <sequence>
                <element name="campaign" nillable="true" type="tns:InteractObject"/>
                <element name="campaignId" nillable="true" type="xsd:long"/>
            </sequence>
        </complexType>
    </element>
    <element name="closeCampaignResponse">
        <complexType>
            <sequence>
                <element name="result" type="xsd:boolean"/>
            </sequence>
        </complexType>
    </element>
    <element name="unscheduleCampaignLaunch">
        <complexType>
            <sequence>
                <element name="campaign" nillable="true" type="tns:InteractObject"/>
                <element name="campaignId" nillable="true" type="xsd:long"/>
                <element name="scheduleDate" nillable="true" type="xsd:dateTime"/>
            </sequence>
        </complexType>
    </element>
    <element name="unscheduleCampaignLaunchResponse">
        <complexType>
            <sequence>
                <element name="result" type="xsd:boolean"/>
            </sequence>
        </complexType>
    </element>
    <simpleType name="FieldType">
        <restriction base="xsd:string">
            <enumeration value="STR500"/>
            <enumeration value="STR4000"/>
            <enumeration value="NUMBER"/>
            <enumeration value="INTEGER"/>
            <enumeration value="TIMESTAMP"/>
